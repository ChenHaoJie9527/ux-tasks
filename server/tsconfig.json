{
    "compilerOptions": {
      "rootDir": "./",
      "outDir": "./dist",
      "module": "ES6",
      "target": "es6",
      "lib": ["es6"],
      "esModuleInterop": true,
      "sourceMap": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"]
      }
    },
    "exclude": [
      "node_modules",
      "**/node_modules",
      "dist",
      "**/dist"
    ],
  }
  